<!--
    Theme capabilities for Aura-es-de
-->

<themeCapabilities>
   
   <themeName>Aura</themeName>

   <aspectRatio>16:9</aspectRatio>
   <aspectRatio>16:10</aspectRatio>
   <aspectRatio>4:3</aspectRatio>


   <aspectRatio>1:1</aspectRatio>
   <aspectRatio>3:2</aspectRatio>
   <aspectRatio>19.5:9</aspectRatio>
   <aspectRatio>21:9</aspectRatio>


   <fontSize>small</fontSize>
   <fontSize>medium</fontSize>
   <fontSize>large</fontSize>
   <fontSize>x-large</fontSize>


   <colorScheme name="art-dark-glass">
      <label>Dark Glass (Art)</label>
   </colorScheme> 
   <colorScheme name="art-light-glass">
      <label>Light Glass (Art)</label>
   </colorScheme>  
   <colorScheme name="art-purple">
      <label>Purple (Art)</label>
   </colorScheme>  
   <colorScheme name="art-blue">
      <label>Blue (Art)</label>
   </colorScheme>
   <colorScheme name="art-green">
      <label>Green (Art)</label>
   </colorScheme>
   <colorScheme name="art-orange">
      <label>Orange (Art)</label>
   </colorScheme>  
   <colorScheme name="controller-purple">
      <label>Purple (Controller)</label>
   </colorScheme>  
   <colorScheme name="controller-blue">
      <label>Blue (Controller)</label>
   </colorScheme>
   <colorScheme name="controller-green">
      <label>Green (Controller)</label>
   </colorScheme>
   <colorScheme name="controller-orange">
      <label>Orange (Controller)</label>
   </colorScheme>
   <colorScheme name="art-grid-purple">
      <label>[Grid] Purple (Art)</label>
   </colorScheme>  
   <colorScheme name="art-grid-blue">
      <label>[Grid] Blue (Art)</label>
   </colorScheme>
   <colorScheme name="art-grid-green">
      <label>[Grid] Green (Art)</label>
   </colorScheme>
   <colorScheme name="art-grid-orange">
      <label>[Grid] Orange (Art)</label>
   </colorScheme>
   <colorScheme name="controller-grid-purple">
      <label>[Grid] Purple (Controller)</label>
   </colorScheme>  
   <colorScheme name="controller-grid-blue">
      <label>[Grid] Blue (Controller)</label>
   </colorScheme>
   <colorScheme name="controller-grid-green">
      <label>[Grid] Green (Controller)</label>
   </colorScheme>
   <colorScheme name="controller-grid-orange">
      <label>[Grid] Orange (Controller)</label>
   </colorScheme>
   <colorScheme name="fs-art-dark-glass">
      <label>[Fullscreen] Dark Glass (Art)</label>
   </colorScheme> 
   <colorScheme name="fs-art-light-glass">
      <label>[Fullscreen] Light Glass (Art)</label>
   </colorScheme>
   <colorScheme name="fs-logo-purple">
      <label>[Fullscreen] Purple (Logo)</label>
   </colorScheme>  
   <colorScheme name="fs-logo-blue">
      <label>[Fullscreen] Blue (Logo)</label>
   </colorScheme>  
   <colorScheme name="fs-logo-green">
      <label>[Fullscreen] Green (Logo)</label>
   </colorScheme>  
   <colorScheme name="fs-logo-orange">
      <label>[Fullscreen] Orange (Logo)</label>
   </colorScheme>

   <transitions name="transitions">
      <label>transitions</label>
      <selectable>false</selectable>
      <systemToSystem>instant</systemToSystem>
      <systemToGamelist>instant</systemToGamelist>
      <gamelistToGamelist>instant</gamelistToGamelist>
      <gamelistToSystem>instant</gamelistToSystem>
      <startupToSystem>instant</startupToSystem>
      <startupToGamelist>instant</startupToGamelist>
   </transitions>
   <suppressTransitionProfiles>
      <entry>builtin-instant</entry>
      <entry>builtin-slide</entry>
      <entry>builtin-fade</entry>
   </suppressTransitionProfiles>

   <variant name="textlist">
      <label>Textlist</label>
      <selectable>true</selectable>
   </variant>
   <variant name="detailed-grid">
      <label>Detailed-Grid</label>
      <selectable>true</selectable>
   </variant>
   <variant name="detailed-grid-help-off">
      <label>Detailed-Grid (Help Off)</label>
      <selectable>true</selectable>
   </variant>
   <variant name="basic-grid">
      <label>Basic-Grid</label>
      <selectable>true</selectable>
   </variant>
   <variant name="basic-grid-help-off">
      <label>Basic-Grid (Help Off)</label>
      <selectable>true</selectable>
   </variant>
   <variant name="carousel-marquee">
      <label>Carousel-Marquee</label>
      <selectable>true</selectable>
   </variant>
   <variant name="carousel-boxart">
      <label>Carousel-Boxart</label>
      <selectable>true</selectable>
   </variant>
</themeCapabilities>
